@PACKAGE_INIT@

find_path(@PROJECT_NAME@_INCLUDE_DIRS NAMES freestdc.h HINTS ${PACKAGE_PREFIX_DIR}/include/freestdc/)
find_library(@PROJECT_NAME@_LIBRARY NAMES freestdc HINTS ${PACKAGE_PREFIX_DIR}/lib/@PROJECT_NAME@/${CMAKE_SYSTEM_PROCESSOR}-${CMAKE_SYSTEM_NAME}/)

include(${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake)

include(FindPackageHandleStandardArgs)
find_package_handle_standard_args(
    FreeSTDC
    REQUIRED_VARS @PROJECT_NAME@_INCLUDE_DIRS @PROJECT_NAME@_LIBRARY)

check_required_components("@PROJECT_NAME@")
set(@PROJECT_NAME@_FOUND 1)
